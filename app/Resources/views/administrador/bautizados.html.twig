{% extends 'base.html.twig' %}

{% block body %}
<h3 class="alert alert-info">Ventana de Miembros Bautizados</h3>

<div class="container">
	<div class="row">
		<div class="col-md-12">
			<a class="btn btn-danger" href="{{path('seleccionarMiembro')}}">
				<i class="fa fa-plus"></i> Nuevo miembro
			</a>
			<br>

			<div class="panel panel-primary ">
				<div class="panel-body">
					<table class=" table table-bordered">
						<tr>
							<th>IdMiembro</th>
							<th>Fecha</th>
							<th>Id Miembro</th>
						</tr>

						<tbody>
							{% for bautizo in Bautizo %}
							<tr>
								<td>{{bautizo.IdBautizo}}</td>
								<td>{{bautizo.Fecha|date('d/m/Y')}} </td>

							</tr>    
							{% endfor %}
						</tbody>
					</table>
				</div>
			</div>	
		</div>
	</div>
</div>


{% endblock %}